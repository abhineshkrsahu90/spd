{
  "version": 2,
  "framework": null,
  "installCommand": null,
  "buildCommand": "cd frontend && mv vite.config.mjs vite.config.ts && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "env": {
    "VITE_API_BASE_URL": "https://your-backend-api.com/api"
  },
  "routes": [
    {
      "src": "/index.html",
      "dest": "/index.html"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ]
}