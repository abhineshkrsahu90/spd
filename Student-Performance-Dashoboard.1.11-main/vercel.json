{
  "version": 2,
  "framework": null,
  "buildCommand": "cd frontend && npm ci --prefer-offline && npx -y vite build && cd ..",
  "outputDirectory": "frontend/dist",
  "installCommand": null,
  "nodeVersion": "18.x",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}